  {
   "cell_type": "code",
   "execution_count": 39,
   "metadata": {},
   "outputs": [],
   "source": [
    "x_mean = np.mean(x_train_reshape)\n",
    "x_std = np.std(x_train_reshape)\n",
    "\n",
    "eps = 1e-10 #In certain cases, small std will lead to unstability in computations. Hence, eps+std\n",
    "x_train_norm = (x_train_reshape - x_mean)/(x_std + eps)\n",
    "x_test_norm = (x_test_reshape - x_mean)/(x_std + eps)"
