   ],
   "source": [
    "#The pics are 28x28--> unroll to 728 pixels\n",
    "import numpy as np\n",
    "\n",
    "x_train_reshape = np.reshape(x_train,(60000,784))\n",
    "x_test_reshape = np.reshape(x_test,(10000,784))\n",
    "print(x_train_reshape.shape)\n",
    "print(x_test_reshape.shape)"
   ]
